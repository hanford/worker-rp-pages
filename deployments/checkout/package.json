    "nsm-fork": "workspace:^0.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.0",
    "@types/node": "^18.6.3",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "typescript": "^4.7.4"
  }
}
